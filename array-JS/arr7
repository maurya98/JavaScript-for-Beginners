/**
 * Calculates the sum of squares of the elements in the input array.
 *
 * @param {Array<number>} a - The array containing the elements to square and sum.
 * @returns {number} The sum of squares of the elements in the input array.
 */
const calculateSumOfSquares = (a) => {
  let sum = 0;
  a.forEach((element) => {
    sum += element * element;
  });
  return sum;
};

module.exports = calculateSumOfSquares;
